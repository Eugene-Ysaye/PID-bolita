//falta la parte d sensores xd
#include <Servo.h>

Servo motorServo;
//convierte al servo en un objeto o algo asi

#define servoPin 9;
#define ultrasoundSensor 14;
#define echoPin 11;
#define triggerPin 12;

int sensorRead;
int angle;

void setup(){
	motorServo.attach(servoPin);
	//ServoPin->motor
	//en algun lado ya tenia pal griton
}

void loop(){
	sensorRead = analogRead(echo);
	angle = map(sensorRead, 0, 255, 0, 180);
	//creo era 0 - 255¿¿¿
	motorServo.write(angle);
}
